№9
symbol = input('Enter symbol:')
while symbol !='A':
    print('Код есть только для символа А, введите повторно')
    symbol = input('Enter symbol:')
else:
    if symbol == 'A':
        symbol = ord('A')
    print(symbol)

№10
symbol = int(input ('Введи символ:'))
while symbol !=65:
    print('Буква есть только для кода 65, введите повторно')
    symbol = int(input('Введи символ:'))
else:
    if symbol == 65:
        symbol = chr(65)
    print(symbol)
    
№11
string1 = ('hole in the ground')
string2 = ('In a hole in the ground there lived a hobbit.')
print (string1 + string2)
count = string1.count(string2)
if count == 0:
    print('YES')
else:
    count == 1
    print('NO')
    
    
